{
    "python.linting.enabled": true,
    "python.analysis.typeshedPaths": [
        ".vscode\\Pico-W-Stub"
    ],
    "python.languageServer": "Pylance",
    "python.analysis.typeCheckingMode": "basic",
    "python.analysis.extraPaths": [
        ".vscode\\Pico-W-Stub\\stubs"
    ],
    "picowgo.fastUpload": false,
    "picowgo.syncFolder": "",
    "picowgo.openOnStart": true,
    "picowgo.pyIgnore": [
        ".picowgo",
        ".vscode",
        ".gitignore",
        ".git",
        "project.pico-go",
        "env",
        "venv",
        "client",
        "package.json",
        "package-lock.json"
    ],
    "picowgo.syncFileTypes": "py,txt,log,json,xml,html,js,css,mpy,svg,png,gz,js.gz",
    "cSpell.words": [
        "ntptime",
        "pimoroni",
        "ssid",
        "uasyncio",
        "Warmz",
        "wifi",
        "wlan"
    ],
    "editor.indentSize": "tabSize",
    "editor.tabSize": 4
}